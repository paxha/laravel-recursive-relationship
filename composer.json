{
    "name": "paxha/laravel-recursive-relationships",
    "description": "This Laravel Eloquent extension provides recursive relationships using common table.",
    "type": "laravel",
    "license": "MIT",
    "keywords": [
        "laravel",
        "laravel library",
        "laravel recursive relationships",
        "recursive relationships",
        "parent child relations"
    ],
    "authors": [
        {
            "name": "Hassan Raza Pasha",
            "email": "e.hassanrazapasha@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.4",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "RecursiveRelationships\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RecursiveRelationships\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
